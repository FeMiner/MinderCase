{
    "name": "kityminder-editor",
    "version": "1.0.61",
    "authors": [
        "fex<fex@baidu.com>"
    ],
    "description": "Kity Minder Editor",
    "main": [
        "dist/kityminder.editor.js",
        "dist/kityminder.editor.css"
    ],
    "keywords": [
